local StartButtonCorner = Instance.new("UICorner")
StartButtonCorner.CornerRadius = UDim.new(0, 20)
StartButtonCorner.Parent = StartButton

-- Phần chức năng: Hiển thị bảng chức năng sau khi chạy loader()
local FunctionFrame = Instance.new("Frame")
FunctionFrame.Name = "FunctionFrame"
FunctionFrame.Size = UDim2.new(0.375, 0, 0.35, 0)
FunctionFrame.Position = UDim2.new(0.3075, 0, 0.67, 0)
FunctionFrame.BackgroundColor3 = Color3.fromRGB(41, 201, 41)
FunctionFrame.BorderSizePixel = 0
FunctionFrame.Visible = false
FunctionFrame.Active = true
