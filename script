FunctionCorner.Parent = FunctionFrame

local FunctionTitle = Instance.new("TextLabel")
FunctionTitle.Name = "FunctionTitle"
FunctionTitle.Size = UDim2.new(1, 0, 0.3, 0)
FunctionTitle.Position = UDim2.new(0, 0, 0.05, 0)
